#!/Users/manavarthivenkat/Desktop/project/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'clodsa==1.2.47','console_scripts','clodsa-sample'
__requires__ = 'clodsa==1.2.47'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('clodsa==1.2.47', 'console_scripts', 'clodsa-sample')()
    )
